{
  "name": "dutch-tax-box3-calculator",
  "version": "0.1.9",
  "description": "A calculator library for Dutch income tax Box 3 calculations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node build.js",
    "test": "npm run build && node --test",
    "example": "node example.js",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shreyasnikte/dutch-tax-box3-calculator-npm.git"
  },
  "keywords": [
    "dutch-tax",
    "box3",
    "calculator",
    "netherlands"
  ],
  "author": "Shreyas Nikte <shreyas.nikte@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.57.1",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3",
    "eslint-config-prettier": "^9.0.0"
  }
}
